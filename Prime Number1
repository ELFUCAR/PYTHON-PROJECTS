'''
PRIME NUMBER

'''
def is_prime(n):

    prime=True
    for i in range(2,n//2+1):

        if n%i==0:
            prime=False
           
    return prime



def primes_in_range(a,b):
    total = 0
    
    for i in range (a,b+1):
      if is_prime(i) == True:
          #print (i)
          total +=1
    print (total)


N=100
print("2 - 1000:", primes_in_range(2,N))

for i in range (1,10):
    
    print(i*N,(i+1)*N,primes_in_range(i*N,(i+1)*N))
    
    
    
